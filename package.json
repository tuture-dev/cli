{
  "name": "tuture-cli",
  "version": "0.3.0",
  "description": "Command-line tool for Tuture.",
  "main": "lib/index.js",
  "preferGlobal": true,
  "keywords": [
    "cli",
    "git",
    "yml"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint -p .",
    "test": "rm -rf lib && tsc && jest",
    "build": "rm -rf lib && tsc",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tutureproject/cli.git"
  },
  "license": "MIT",
  "bin": {
    "tuture": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/tutureproject/cli/issues"
  },
  "homepage": "https://github.com/tutureproject/cli#readme",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.1",
    "figures": "^2.0.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.12.0",
    "minimatch": "^3.0.4",
    "parse-diff": "^0.4.2",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^1",
    "@types/figures": "^2.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.42",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/minimatch": "^3.0.3",
    "@types/tmp": "^0.0.33",
    "@types/which": "^1.3.1",
    "jest": "^23.4.2",
    "tmp": "^0.0.33",
    "ts-jest": "^23.1.2",
    "tslint": "^5",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9"
  },
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tuture",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  }
}
